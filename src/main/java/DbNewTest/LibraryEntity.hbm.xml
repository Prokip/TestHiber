<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DbNewTest.LibraryEntity" table="library" schema="public" catalog="dbtest">
        <id name="bookId">
            <column name="book_id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="author">
            <column name="author" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="numofpage">
            <column name="numofpage" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>